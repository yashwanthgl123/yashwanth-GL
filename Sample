public class Sample
{
    public static void main (String []args)
    {
        int[] arr = new int[5];
        arr[0]=1;
        arr[1]=2;
        arr[2]=3;
        arr[3]=4;
        arr[4]=5;
        int sum=0;
        for(int x:arr)
        {
           sum=sum + x;
            }
            System.out.println(sum);
        }
    }
